                                        a.blog-card-link.button(href="/")
                                            span.icon.icon-yellow-arrow View Blog
                                li.list-item
                                    article.blog-card.container
                                        div.blog-card-author.person-card
                                            img.person-card-image(src="./images/team/3.png", alt="", width="80", height="80")
                                            div.person-card-body
                                                p.person-card-name Sarah Ethicist
                                                p.person-card-department AI Ethics
                                        div.blog-card-body
                                            time.blog-card-date(datetime="2023-10-15") November 5, 2023
                                            div.blog-card-info
                                                h4.blog-card-title The Ethical Dilemmas of AI
                                                div.blog-card-description
