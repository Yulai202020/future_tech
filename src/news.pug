                                                a.new-card-link.button(href="/")
                                                    span.icon.icon-yellow-arrow Read More
                                    li.news-item
                                        acticle.news-card.container
                                            img.news-card-image(src="./images/news/4.jpg", alt="", width="512", height="222")
                                            div.news-card-body
                                                h2.news-card-title.h6 COVID-19 Variants
                                                div.news-card-desciption
                                                    p Health
                                            div.news-card-controls
                                                div.news-card-actions.blog-actions
